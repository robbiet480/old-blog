---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Robbie Trencheny</title>
 <link href="http://robbie.io/atom.xml" rel="self"/>
 <link href="http://robbie.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://robbie.io/</id>
 <author>
   <name>Robbie Trencheny</name>
   <email>me@robbiet.us</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://robbie.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://robbie.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>